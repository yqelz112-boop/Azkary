{
  "114": {
    "name_ar": "الناس",
    "name_en": "An-Nas",
    "verses": [
      { "number": 1, "text": "قُلْ أَعُوذُ بِرَبِّ النَّاسِ" },
      { "number": 2, "text": "مَلِكِ النَّاسِ" },
      { "number": 3, "text": "إِلَٰهِ النَّاسِ" },
      { "number": 4, "text": "مِنْ شَرِّ الْوَسْوَاسِ الْخَنَّاسِ" },
      { "number": 5, "text": "الَّذِي يُوَسْوِسُ فِي صُدُورِ النَّاسِ" }
    ]
  },
  "112": {
    "name_ar": "الإخلاص",
    "name_en": "Al-Ikhlas",
    "verses": [
      { "number": 1, "text": "قُلْ هُوَ اللَّهُ أَحَدٌ" },
      { "number": 2, "text": "اللَّهُ الصَّمَدُ" },
      { "number": 3, "text": "لَمْ يَلِدْ وَلَمْ يُولَدْ" },
      { "number": 4, "text": "وَلَمْ يَكُنْ لَهُۥۤۚ كُفُوًا أَحَدٌ" }
    ]
  },
  "113": {
    "name_ar": "الفلق",
    "name_en": "Al-Falaq",
    "verses": [
      { "number": 1, "text": "قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ" },
      { "number": 2, "text": "مِنْ شَرِّ مَا خَلَقَ" },
      { "number": 3, "text": "وَمِنْ شَرِّ غَاسِقٍ إِذَا وَقَبَ" },
      { "number": 4, "text": "وَمِنْ شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ" },
      { "number": 5, "text": "وَمِنْ شَرِّ حَاسِدٍ إِذَا حَسَدَ" }
    ]
  }
}
